<!-- Root LinearLayout -->
<!-- Defines the root layout with vertical orientation, taking up the full width and height of the screen. -->
<!-- The 'tools:context' attribute specifies the associated Kotlin activity for design-time tools. -->

<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".view.EventsLogActivity">


</LinearLayout>
